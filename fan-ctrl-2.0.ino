  </div>
  <p><a href="https://www.github.com/agrabbs/" target="_blank">README.md</a> | <a href="https://www.buymeacoffee.com/andrewgrabbs" target="_blank">Buy Andrew a &#129360;!</a></p>
</body></html>)rawliteral";

// HTML Processor
String processor(const String& var) PROGMEM {
  if (var == PARAM_INPUT_1) {
    return String(low_value);
  }
  if (var == PARAM_INPUT_2) {
    return String(medium_value);
  }
  if (var == PARAM_INPUT_3) {
    return String(high_value);
  }
  return String();
}

// Web Server 404
void notFound(AsyncWebServerRequest *request) {
  request->send(404, "text/plain", "");
}


void setup() {
  Serial.begin(115200);
  preferences.begin("settings", false);
  unsigned int boot_counter = preferences.getUInt("boot_counter", 0);
  boot_counter++;
  Serial.printf("Boot Count: %u\n", boot_counter);
  preferences.putUInt("boot_counter", boot_counter);
  low_value = preferences.getInt(PARAM_INPUT_1, 0);
  medium_value = preferences.getInt(PARAM_INPUT_2, 0);
  high_value = preferences.getInt(PARAM_INPUT_3, 0);

  WiFi.mode(WIFI_STA);
  WiFi.begin(ssid, password);
  if (WiFi.waitForConnectResult() != WL_CONNECTED) {
    //Serial.println("Error setting up WiFi!");
    return;
  }
  // Initialize mDNS
  if (!MDNS.begin("fan-ctrl")) {
    //Serial.println("Error setting up mDNS!");
    while (1) {
      delay(1000);
    }
  }
  Serial.print("mDNS responder started\nIP Address: ");
  Serial.println(WiFi.localIP());

  // Send Webpage to User
  server.on("/", HTTP_GET, [](AsyncWebServerRequest * request) {
    request->send_P(200, "text/html", index_html, processor);
  });

  // Handle Responses
  server.on("/get", HTTP_GET, [] (AsyncWebServerRequest * request) {
    if (request->hasParam(PARAM_INPUT_1)) {
      int new_low_value = request->getParam(PARAM_INPUT_1)->value().toInt();
      if (new_low_value != low_value) {
        low_value = new_low_value;
        preferences.putInt(PARAM_INPUT_1, low_value);
      }
    }
    if (request->hasParam(PARAM_INPUT_2)) {
      int new_medium_value = request->getParam(PARAM_INPUT_2)->value().toInt();
      if (new_medium_value != medium_value) {
        medium_value = new_medium_value;
        preferences.putInt(PARAM_INPUT_2, medium_value);
      }
    }
    if (request->hasParam(PARAM_INPUT_3)) {
      int new_high_value = request->getParam(PARAM_INPUT_3)->value().toInt();
      if (new_high_value != high_value) {
        high_value = new_high_value;
        preferences.putInt(PARAM_INPUT_3, high_value);
      }
    }
    request->send_P(200, "text/html", index_html, processor);
  });
  server.onNotFound(notFound);
  server.begin();
}

void loop() {

}
