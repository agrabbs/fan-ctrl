#include <WiFi.h>
#include <ESPmDNS.h>
#include <Arduino.h>
#include <AsyncTCP.h>
#include <Preferences.h>
#include <ESPAsyncWebServer.h>

Preferences preferences;
AsyncWebServer server(80);

// REPLACE WITH YOUR NETWORK CREDENTIALS
const char* ssid PROGMEM = "";
const char* password PROGMEM = "";

const char* PARAM_INPUT_1 PROGMEM = "low";
const char* PARAM_INPUT_2 PROGMEM = "medium";
const char* PARAM_INPUT_3 PROGMEM = "high";
int low_value;
int medium_value;
int high_value;

// Raw HTML
const char index_html[] PROGMEM = R"rawliteral(
<!DOCTYPE HTML><html><head>
  <title>andrewgrabbs.com - FAN-CTRL v2.0</title>
  <style>html{font-family: Helvetica; display: inline-block; margin:0px auto;text-align:center;}
  body {background-color:#ebebeb;margin-top: 50px;} h1 {color: #444444;margin: 0px auto 30px;} h3 {color: #444444;margin-bottom: 50px;}
  a {color: #000;font-weight:bold;text-decoration:none;}
  .button {display: inline;width: 80px;background-color: #3498db;border: none;color: white;padding: 13px 13px;text-decoration: none;font-size: 15px;margin: 0px auto;cursor: pointer;border-radius: 4px;}
  .button-on {background-color: #3498db;}
  .button-on:active {background-color: #2980b9;}
  .background {position: relative; height:400px;width:600px;margin:0px auto;background-image:url('data:image/png;base64,');}
  .bar {position:absolute;width:600px;height:25px;margin:251px auto;}
  .bar input {position:relative;background-color:rgba(0, 0, 0, 0);text-align:right;font-size:18px;border:0px;width:50px;height:25px;margin:0px 32px;font-weight:bold;}
